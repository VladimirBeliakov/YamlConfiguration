[*.cs]
dotnet_naming_symbols.private_methods_symbols.applicable_kinds = method
dotnet_naming_symbols.private_methods_symbols.applicable_accessibilities = private

dotnet_naming_style.camel_case_style.capitalization = camel_case

dotnet_naming_rule.private_methods_must_be_in_camel_case.symbols = private_methods_symbols
dotnet_naming_rule.private_methods_must_be_in_camel_case.style = camel_case_style
dotnet_naming_rule.private_methods_must_be_in_camel_case.severity = suggestion
